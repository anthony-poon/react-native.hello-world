        "react-native-elements": "^1.1.0",
        "react-native-web": "^0.11.4"
    },
    "devDependencies": {
        "@expo/vector-icons": "latest",
        "babel-preset-expo": "^5.1.1",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.14.0",
        "enzyme-to-json": "^3.4.0",
        "jest-enzyme": "^7.1.0",
        "jest-expo": "^34.0.0",
        "lodash": "latest",
        "lorem-ipsum": "^2.0.3",
        "memoize-one": "^5.0.5",
        "moment": "^2.24.0",
        "prop-types": "^15.7.2",
        "react-dom": "16.8",
